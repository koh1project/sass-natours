  },
  "homepage": "https://github.com/koh1project/sass-natours#readme",
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "concat": "^1.0.3",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.15",
    "postcss-cli": "^8.3.1"
  }
}
